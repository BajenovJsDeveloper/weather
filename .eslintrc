{
	"parser": "babel-eslint",
	"extends":["airbnb/base"],
	"env":{
		"browser":true,
		"es6":true,
		"node":true
	},
	"rules":{
		"no-plusplus":"off",
		"no-console":"off",
		"no-unused-vars":0,
		"no-underscore-dangle":"off",
		"semi": "error",
		'max-len': ['error', 100, 2, { ignoreUrls: true, }],
	},
	"plugins":["babel","html","import","jsx-a11y","react","prettier"],
	  "settings": {
        "html/indent": "tab"
        }
}